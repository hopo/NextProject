<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- ============ sessionMapper.xml =================================================== -->

<mapper namespace="kr.or.nextit.session.service.impl.SessionMapper">

	<!-- 로그인 select -->
	<select id="loginCheck" resultType="kr.or.nextit.comm.model.EmployeeVo"  parameterType="hashMap">
		SELECT
			emp_id,
			emp_pwd,
		    emp_name
		FROM
			tb_employee
		WHERE
			emp_id = #{empId} AND
			emp_pwd=  #{empPwd}
	</select>
	
</mapper>




























